{
  "name": "gatsby-starter-kit",
  "description": "Gatsby with TypeScript",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "env-cmd -f .env.development gatsby develop",
    "generate": "graphql-codegen",
    "lint": "eslint --config .eslintrc.js ./src --ext .ts,.tsx --fix",
    "lint-staged": "lint-staged --config .lintstagedrc.json",
    "prettier": "prettier --config .prettierrc.json --write \"src/**/*.{ts,tsx}\"",
    "start": "npm run develop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@emotion/core": "10.0.17",
    "@types/node": "12.7.8",
    "@types/react": "16.9.3",
    "@types/react-dom": "16.9.1",
    "@types/react-helmet": "5.0.11",
    "babel-plugin-emotion": "10.0.19",
    "fp-ts": "2.0.5",
    "gatsby": "2.15.28",
    "gatsby-plugin-emotion": "4.1.9",
    "gatsby-plugin-react-helmet": "3.1.10",
    "gatsby-plugin-sass": "2.1.17",
    "gatsby-plugin-typescript": "2.1.11",
    "gatsby-plugin-typescript-checker": "1.1.1",
    "gatsby-source-filesystem": "2.1.28",
    "gatsby-transformer-remark": "2.6.26",
    "react": "16.10.1",
    "react-dom": "16.10.1",
    "react-helmet": "5.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.7.0",
    "@graphql-codegen/typescript": "1.7.0",
    "@graphql-codegen/typescript-operations": "1.7.0",
    "@typescript-eslint/eslint-plugin": "2.3.1",
    "@typescript-eslint/parser": "2.3.1",
    "env-cmd": "10.0.1",
    "eslint-config-prettier": "6.3.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "2.1.1",
    "eslint-plugin-simple-import-sort": "4.0.0",
    "eslint-plugin-standard": "4.0.1",
    "gatsby-plugin-eslint": "2.0.5",
    "husky": "3.0.5",
    "lint-staged": "9.2.5",
    "node-sass": "4.12.0",
    "prettier": "1.18.2",
    "ts-node": "8.4.1",
    "typescript": "3.6.3"
  }
}
