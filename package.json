{
  "name": "gatsby-starter-kit",
  "description": "Gatsby with TypeScript",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "env-cmd -f .env.development gatsby develop",
    "generate": "graphql-codegen",
    "lint": "eslint --config .eslintrc.js ./src --ext .ts,.tsx --fix",
    "lint-staged": "lint-staged --config .lintstagedrc.json",
    "prettier": "prettier --config .prettierrc.json --write \"src/**/*.{ts,tsx}\"",
    "start": "npm run develop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@emotion/core": "10.0.17",
    "@types/node": "12.7.3",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "babel-plugin-emotion": "10.0.17",
    "fp-ts": "2.0.5",
    "gatsby": "2.15.2",
    "gatsby-plugin-emotion": "4.1.5",
    "gatsby-plugin-sass": "2.1.13",
    "gatsby-plugin-typescript": "2.1.6",
    "gatsby-plugin-typescript-checker": "1.1.1",
    "gatsby-source-filesystem": "2.1.19",
    "gatsby-transformer-remark": "2.6.20",
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.7.0",
    "@graphql-codegen/typescript": "1.7.0",
    "@graphql-codegen/typescript-operations": "1.7.0",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "env-cmd": "10.0.1",
    "eslint-config-prettier": "6.1.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "9.2.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "2.0.1",
    "eslint-plugin-standard": "4.0.1",
    "gatsby-plugin-eslint": "2.0.5",
    "graphql-codegen": "0.4.0",
    "husky": "3.0.5",
    "lint-staged": "9.2.5",
    "node-sass": "4.12.0",
    "prettier": "1.18.2",
    "ts-node": "8.3.0",
    "typescript": "3.6.2"
  }
}
