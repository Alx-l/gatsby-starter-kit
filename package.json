{
  "name": "gatsby-starter-kit",
  "description": "Gatsby with TypeScript",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "env-cmd -f .env.development gatsby develop",
    "generate": "graphql-codegen",
    "lint": "eslint --config .eslintrc.js ./src --ext .ts,.tsx --fix",
    "lint-staged": "lint-staged --config .lintstagedrc.json",
    "prettier": "prettier --config .prettierrc.json --write \"src/**/*.{ts,tsx}\"",
    "start": "npm run develop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@emotion/core": "10.0.22",
    "@types/node": "12.12.14",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "@types/react-helmet": "5.0.14",
    "babel-plugin-emotion": "10.0.23",
    "fp-ts": "2.0.5",
    "gatsby": "2.18.4",
    "gatsby-plugin-emotion": "4.1.16",
    "gatsby-plugin-react-helmet": "3.1.16",
    "gatsby-plugin-sass": "2.1.24",
    "gatsby-plugin-typescript": "2.1.20",
    "gatsby-plugin-typescript-checker": "1.1.1",
    "gatsby-source-filesystem": "2.1.39",
    "gatsby-transformer-remark": "2.6.38",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.9.0",
    "@graphql-codegen/typescript": "1.9.0",
    "@graphql-codegen/typescript-operations": "1.9.0",
    "@typescript-eslint/eslint-plugin": "2.9.0",
    "@typescript-eslint/parser": "2.9.0",
    "env-cmd": "10.0.1",
    "eslint-config-prettier": "6.7.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "eslint-plugin-simple-import-sort": "5.0.0",
    "eslint-plugin-standard": "4.0.1",
    "gatsby-plugin-eslint": "2.0.8",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "node-sass": "4.13.0",
    "prettier": "1.19.1",
    "ts-node": "8.5.2",
    "typescript": "3.7.2"
  }
}
