{
  "name": "gatsby-starter-kit",
  "description": "Gatsby with TypeScript",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "env-cmd -f .env.development gatsby develop",
    "generate": "graphql-codegen",
    "lint": "eslint --config .eslintrc.js ./src --ext .ts,.tsx --fix",
    "lint-staged": "lint-staged --config .lintstagedrc.json",
    "prettier": "prettier --config .prettierrc.json --write \"src/**/*.{ts,tsx}\"",
    "start": "npm run develop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@emotion/core": "10.0.14",
    "@types/node": "12.0.12",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "babel-plugin-emotion": "10.0.14",
    "fp-ts": "2.0.1",
    "gatsby": "2.13.6",
    "gatsby-plugin-emotion": "4.1.0",
    "gatsby-plugin-sass": "2.1.0",
    "gatsby-plugin-typescript": "2.1.0",
    "gatsby-plugin-typescript-checker": "1.1.1",
    "gatsby-source-filesystem": "2.1.2",
    "gatsby-transformer-remark": "2.6.1",
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.3.1",
    "@graphql-codegen/typescript": "1.3.1",
    "@graphql-codegen/typescript-operations": "1.3.1",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "env-cmd": "9.0.3",
    "eslint-config-prettier": "6.0.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-react-hooks": "1.6.1",
    "eslint-plugin-standard": "4.0.0",
    "gatsby-plugin-eslint": "2.0.5",
    "graphql-codegen": "0.4.0",
    "husky": "3.0.0",
    "lint-staged": "9.1.0",
    "node-sass": "4.12.0",
    "prettier": "1.18.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.2"
  }
}
